public class VcfLoaderException : System.Exception
{
    public VcfLoaderException() { }
    public VcfLoaderException(string message) : base(message) { }
    public VcfLoaderException(string message, System.Exception inner) : base(message, inner) { }
    protected VcfLoaderException(
        System.Runtime.Serialization.SerializationInfo info,
        System.Runtime.Serialization.StreamingContext context) : base(info, context) { }
}